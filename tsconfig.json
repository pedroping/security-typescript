{
  "compilerOptions": {
    "module": "commonjs",
    "noImplicitAny": true,
    "removeComments": true,
    "preserveConstEnums": true,
    "sourceMap": true,
    "declaration": true,
    "moduleResolution": "node",
    "outDir": "./dist",
    "rootDir": ".",
    "esModuleInterop": true,
    "typeRoots": ["./src/cache-handle/index.d.ts"],
    "paths": {
      "@cache-handle": ["./src/cache-handle/cache-handle.ts"]
    }
  },
  "include": ["src/**/*", "html.d.ts"],
  "exclude": ["node_modules"]
}
